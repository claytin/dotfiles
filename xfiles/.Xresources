! special
*.foreground:  #6a7474
*.background:  #fdf6e3
*.cursorColor: #6a7474

! black
*.color0:      #5e5c65
*.color8:      #6e6b76

! red
*.color1:      #f26371
*.color9:      #ea737f

! green
*.color2:      #86c486
*.color10:     #99cc99

! yellow
*.color3:      #c9a764
*.color11:     #d3b069

! blue
