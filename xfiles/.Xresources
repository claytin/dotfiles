! special
*.foreground:   #f0f0f0
*.background:   #151515
*.cursorColor:  #f0f0f0

! black
*.color0:       #151515
*.color8:       #505558

! red
*.color1:       #c75049
*.color9:       #e86869

! green
*.color2:       #9cb33b
*.color10:      #cdee69

! yellow
*.color3:       #d0b03c
*.color11:      #ffe377

! blue
*.color4:       #72b3cc
*.color12:      #9cd9f0

! magenta
*.color5:       #c9a0d0
*.color13:      #fbb1f9

! cyan
