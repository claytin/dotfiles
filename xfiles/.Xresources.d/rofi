! enabled modi
rofi.modi:                           run,window
! window width
rofi.width:                          35
! number of lines
rofi.lines:                          10

! font to use
rofi.font:                           tamsyn 10

! border width
rofi.bw:                             2

! "Whether to load and show icons" Set from: Default
! rofi.show-icons:                     false
! "Theme to use to look for icons" Set from: Default
! rofi.drun-icon-theme:

! terminal to use
rofi.terminal:                       urxvtc

! disable history in run/ssh
rofi.disable-history:                true

! "Ssh client to use" Set from: Default
! rofi.ssh-client:                     ssh
! "Ssh command to execute" Set from: Default
! rofi.ssh-command:                    {terminal} -e {ssh-client} {host}

! command to execute that runs in shell
rofi.run-shell-command:              {terminal} -name {cmd} -e {cmd}

! use sorting
rofi.sort:                           true
! set case-sensitivity
rofi.case-sensitive:                 true

! "Parse hosts file for ssh mode" Set from: Default
! rofi.parse-hosts:                    false
! "Parse known_hosts file for ssh mode" Set from: Default
! rofi.parse-known-hosts:              true

! "Set the modi to combine in combi mode" Set from: Default
! rofi.combi-modi:                     window,run
! set the matching algorithm. (normal, regex, glob, fuzzy)
rofi.matching:                       fuzzy
! "Tokenize input string" Set from: Default
! rofi.tokenize:                       true
! threads to use for string matching
rofi.threads:                        4

! hide scroll-bar *DEPRECATED*
rofi.hide-scrollbar:                 true
! scrolling method. (0: Page, 1: Centered)
rofi.scroll-method:                  1

! (1: odd lines, 2: foreground, 3: even lines, 4: selection, 5: selected fg)
! color scheme for normal row
rofi.color-normal:                   base00, base04, base01, base02, base04

! (1: odd , 2: foreground, 3: even, 4: selection 5: selected fg)
! color scheme for active window row
rofi.color-active:                   base00, base0b, base01, base02, base0b

! (1: odd , 2: foreground, 3: even, 4: selection 5: selected fg)
! color scheme for urgent row
rofi.color-urgent:                   base00, base08, base01, base02, base08

! (1: window, 2: border, 3: separator)
! window
rofi.color-window:                   base00, #5d6666, #5d6666

